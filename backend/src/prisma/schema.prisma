generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int    @id @default(autoincrement())
  name      String
  email     String @unique
  password  String @default("")
  sessions  User_Session[]
}

model User_Session {
  id        Int @id @default(autoincrement())
  user      User @relation(fields: [user_id], references: [id])
  user_id   Int
  token     String @unique
}

model Car {
  id    Int    @id @default(autoincrement())
  name  String
  image String
  price String
}
